generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root@localhost:3306/dblogin"
}

model User {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  birthday   DateTime
  name       String
  password   String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("tb_user")
}
